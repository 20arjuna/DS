public class Fibber
{
  private isFirst;
  private prev;
  private current;
  public Fibber()
  {
    isFirst = true;
    prev = 0;
    current = 1;
  }
  public next()
  {
    if(isFirst)
    {
      isFirst = false;
      return 1;
    }
    next = prev + current;
    prev = current;
    current = next;
    return next;
  }
}
